{
  "name": "tracinglog",
  "rules": {
    "logs": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_logline"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          },
          {
            "type": "SYMBOL",
            "name": "comment"
          }
        ]
      }
    },
    "_logline": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "log_date"
        },
        {
          "type": "SYMBOL",
          "name": "log_time"
        },
        {
          "type": "SYMBOL",
          "name": "level"
        },
        {
          "type": "SYMBOL",
          "name": "module_name"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "important"
          }
        }
      ]
    },
    "log_date": {
      "type": "SYMBOL",
      "name": "year_month_date"
    },
    "log_time": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "time_with_offset"
        },
        {
          "type": "SYMBOL",
          "name": "time_without_offset"
        }
      ]
    },
    "year_month_date": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "([0-9]+)-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])"
          },
          {
            "type": "PATTERN",
            "value": "[ tT]"
          }
        ]
      }
    },
    "time_with_offset": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "PATTERN",
            "value": "([01][0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9]|60)([.][0-9]+)?"
          },
          {
            "type": "PATTERN",
            "value": "([zZ])|([+-]([01][0-9]|2[0-3]):[0-5][0-9])"
          }
        ]
      }
    },
    "time_without_offset": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "([01][0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9]|60)([.][0-9]+)?"
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": "[^\\n]+"
          },
          {
            "type": "PATTERN",
            "value": "\\r?\\n"
          }
        ]
      }
    },
    "level": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bad_level"
        },
        {
          "type": "SYMBOL",
          "name": "ok_level"
        }
      ]
    },
    "bad_level": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "WARN"
        },
        {
          "type": "STRING",
          "value": "ERROR"
        },
        {
          "type": "STRING",
          "value": "FATAL"
        }
      ]
    },
    "ok_level": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "DEBUG"
        },
        {
          "type": "STRING",
          "value": "INFO"
        },
        {
          "type": "STRING",
          "value": "TRACE"
        }
      ]
    },
    "module_name": {
      "type": "PATTERN",
      "value": "\\s?[-_a-zA-Z0-9:]*"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "\""
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "IMMEDIATE_TOKEN",
                "content": {
                  "type": "PREC",
                  "value": 1,
                  "content": {
                    "type": "PATTERN",
                    "value": "[^\"\\n]+"
                  }
                }
              }
            },
            {
              "type": "STRING",
              "value": "\""
            }
          ]
        }
      ]
    },
    "important": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "constant"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    },
    "_uuid": {
      "type": "PATTERN",
      "value": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
    },
    "_ipv4": {
      "type": "PATTERN",
      "value": "([0-9]{1,3}\\.){3}[0-9]{1,3}(\\/([0-9]|[1-2][0-9]|3[0-2]))?"
    },
    "constant": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_ipv4"
        },
        {
          "type": "SYMBOL",
          "name": "_uuid"
        },
        {
          "type": "PATTERN",
          "value": "\\s\\d+"
        },
        {
          "type": "PATTERN",
          "value": "true|false"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

